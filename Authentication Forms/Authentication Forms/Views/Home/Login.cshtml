
@{
    ViewBag.Title = "Login";
}
@model Authentication_Forms.Models.Signup
<h2>Login</h2>

@Html.Raw(TempData["usernotfound"])

@using (Html.BeginForm("Login", "Home"))
{
    @Html.Label("Email: ")
    @Html.TextBox("email")
    <br />
    @Html.Label("Password: ")
    @Html.TextBox("password")
    <br />
    <input type="submit" value="submit" />

}
